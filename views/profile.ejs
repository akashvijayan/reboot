									<a href="/">Home</a>
									<a href="/videogallery">Videos</a>
									<a href="/tools">Tools</a>
									<% if(!userData) { %>
									<a href="/login">Login</a>
									<% } else {%>
									<a href="/logout">Logout</a>
									<% } %>
								</nav>
								<div class="menu-bar"><span class="lnr lnr-menu"></span></div>
							</div>
						</div>
					</div>
				</div>
			</header>
			<div>
				<div class="overlay overlay-bg"></div>
				<div class="container">
					<div class="row height align-items-center justify-content-center">
						<div class="col-lg-7">
							<div class="banner-content text-center">
								<h2>Profile</h2>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
	<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<!------ Include the above in your HEAD tag ---------->


	<div class="main-wrapper">
		<!-- Start Feature Area -->

		<!-- End Feature Area -->


		<!-- Start service Area -->

		<section class="service-area">
			<div class="container">
				<form>
					<div class="form-group row">
						<label class="col-sm-2 col-form-label">username</label>
						<div class="col-sm-10">
							<input type="email" class="form-control" value="<%=u.username%>" disabled>
						</div>
					</div>
					<div class="form-group row">
						<label class="col-sm-2 col-form-label">name</label>
						<div class="col-sm-10">
							<input type="email" class="form-control" value="<%=u.name%>" disabled>
						</div>
					</div>
					<h5 class="my-5">Group Members</h5>
					<div class="row">
						<% for(var i = 0; i<u.groupMembers.length-1;i++) { %>
						<div class="col-lg-3 card" style="width: 11rem;">
							<div class="card-body">
								<h5 class="card-title"><%= u.groupMembers[i] %></h5>
								<h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
								<p class="card-text">Some quick example text to build on the card title and make up the
									bulk of the card's content.</p>
							</div>
						</div>
						<% } %>
					</div>
				</form>

				<button onclick="plot()" class="btn btn-secondary my-3">Plot Stats Graph</button>

				<div id='myDiv'>
					<!-- Plotly chart will be drawn inside this DIV -->
				</div>
			</div>
		</section>
		<!-- End service Area -->


		<!-- Start footer Area -->
		<footer class="footer-area relative">
			<div class="container">
