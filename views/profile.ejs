	<script src='https://cdn.plot.ly/plotly-latest.min.js'></script>

	<script>
		function plot() {
			var trace2 = {
				x: [2, 3, 4, 5],
				y: [5, 1, 4, 5],
				mode: 'lines',
				type: 'scatter'
			};
			var line = [trace2]
			Plotly.newPlot('myDiv', line);

			// Plotly.plot()
		}
	</script>
